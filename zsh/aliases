# Aliases

# Basic Commands
alias cls='clear'
alias l='ls'

## Git
alias ga='git add -A'
alias gd='git diff'
alias gdc='git diff --cached'
alias gc='git commit'
alias gcm='git commit -m '
alias gp='git push'
alias gst='git status'
alias gco='git checkout'
alias gb='git branch'

## Vim
alias vi='vim'
alias nvim='vim'
alias vi='vim'

## Mistakes
alias gs='echo "Ghostscript has been remapped to ghostscript"'
alias ghostscript='/usr/local/bin/gs'

