export DOTFILES=$HOME/.dotfiles
ZSH_MODULES=$DOTFILES/zsh/modules

# Path Exports


# Prompt
source $DOTFILES/zsh/prompt/spaceship.zsh

# ZSH Modules
# --------------------------
# Path Exports
source $ZSH_MODULES/path.zsh
# Aliases
source $ZSH_MODULES/aliases.zsh
# Colors
source $ZSH_MODULES/colors.zsh
# Functions
source $ZSH_MODULES/functions.zsh
# Secrets (*not committed*)
source $ZSH_MODULES/secrets.zsh
# Git Completions
source $ZSH_MODULES/completions/completions.zsh

