{
  "window.commandCenter": false,
  "editor.tabSize": 2,
  "workbench.activityBar.orientation": "vertical",
  "workbench.colorTheme": "GitHub Dark Colorblind (Beta)",
  "window.titleBarStyle": "custom",
  "editor.fontFamily": "JetBrainsMono Nerd Font",
  "window.zoomLevel": 1,
  "cursor.chat.terminalShowHoverHint": false,
  "cursor.cmdk.autoSelect": false,
  "cursor.composer.agentLoopOnLints": false,
  "editor.minimap.enabled": true,
  "editor.minimap.renderCharacters": false,
  "editor.fontLigatures": true,
  "workbench.panel.defaultLocation": "right",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vim.handleKeys": {
    "<C-p>": false,
    "<C-w>": false,
    "<C-c>": false,
    "<C-v>": false,
    "<C-f>": false,
    "<CapsLock>": false
  },
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    },
    {
      "before": ["k", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.useSystemClipboard": true,
  "chat.commandCenter.enabled": false,
  "workbench.layoutControl.enabled": false,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "explorer.confirmDelete": false,
  "git.openRepositoryInParentFolders": "never",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "chat.agent.enabled": false,
  "chat.focusWindowOnConfirmation": false,
  "telemetry.feedback.enabled": false,
  "workbench.editor.empty.hint": "hidden",
  "git.blame.editorDecoration.enabled": true,
  "workbench.productIconTheme": "material-product-icons",
  "workbench.iconTheme": "material-icon-theme",
  "latex-workshop.hover.command.enabled": false,
  "cSpell.userWords": ["checkpointer", "langchain", "langfuse", "Mjml"],
  "workbench.startupEditor": "none",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "builder.serverUrl": "http://localhost:5000",
  "builder.command": "docker compose up",
  "builder.openInBrowser": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "update.showReleaseNotes": false,
  "[dockercompose]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "git.blame.statusBarItem.enabled": true
}
